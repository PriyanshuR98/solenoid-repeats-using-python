matrix = list()

for a in range(len(sequence)):
    s = sequence[a]
    vec1 = list()
    vec2 = list()
    vec3 = list()
    vec4 = list()
    vec5 = list()
    for i in range(len(s)):
        vec1.append((dicfact1[s[i]]))
        vec2.append((dicfact2[s[i]]))
        vec3.append((dicfact3[s[i]]))
        vec4.append((dicfact4[s[i]]))
        vec5.append((dicfact5[s[i]]))
    
    res = []
    res.append(vec1)
    res.append(vec2)
    res.append(vec3)
    res.append(vec4)
    res.append(vec5)    
    matrix.append(res)


